# ============================================================
# DoAi Gateway - Workstation 2 Configuration
# ============================================================
# Configuration for the second workstation deployment.
# Uses different port to allow multiple gateways on same machine.
#
# Usage: start-gateway.bat workstation-2.env
#        .\start-gateway.ps1 -Config workstation-2.env
#        ./start-gateway.sh workstation-2.env
#
# ============================================================

# -----------------------------------------------------------
# Server Settings
# -----------------------------------------------------------
# Different port from workstation-1 to avoid conflicts
PORT=3101
NODE_ID=workstation-2

# -----------------------------------------------------------
# Laixi Connection
# -----------------------------------------------------------
LAIXI_ENABLED=true
LAIXI_URL=ws://127.0.0.1:22221/
LAIXI_TIMEOUT=10000
LAIXI_HEARTBEAT_INTERVAL=5000

# -----------------------------------------------------------
# Cloud Gateway Connection
# -----------------------------------------------------------
VULTR_ENABLED=true
VULTR_URL=ws://158.247.210.152:8000/ws/node
NODE_SECRET=

# -----------------------------------------------------------
# ADB Settings
# -----------------------------------------------------------
ADB_HOST=127.0.0.1
ADB_PORT=5037

# -----------------------------------------------------------
# Chrome Automation
# -----------------------------------------------------------
CHROME_ENABLED=false
CHROME_BASE_PORT=9400
CHROME_MAX_CONNECTIONS=50

# -----------------------------------------------------------
# Logging
# -----------------------------------------------------------
LOG_LEVEL=info
